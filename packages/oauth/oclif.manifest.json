{
  "version": "7.68.0",
  "commands": {
    "authorizations:create": {
      "id": "authorizations:create",
      "description": "create a new OAuth authorization",
      "strict": true,
      "pluginName": "@heroku-cli/plugin-oauth",
      "pluginAlias": "@heroku-cli/plugin-oauth",
      "pluginType": "core",
      "aliases": [],
      "examples": [
        "$ heroku authorizations:create --description \"For use with Anvil\""
      ],
      "flags": {
        "description": {
          "name": "description",
          "type": "option",
          "char": "d",
          "description": "set a custom authorization",
          "multiple": false
        },
        "short": {
          "name": "short",
          "type": "boolean",
          "char": "S",
          "description": "only output token",
          "allowNo": false
        },
        "json": {
          "name": "json",
          "type": "boolean",
          "char": "j",
          "description": "output in json format",
          "allowNo": false
        },
        "scope": {
          "name": "scope",
          "type": "option",
          "char": "s",
          "description": "set custom OAuth scopes",
          "multiple": false
        },
        "expires-in": {
          "name": "expires-in",
          "type": "option",
          "char": "e",
          "description": "set expiration in seconds (default no expiration)",
          "multiple": false
        }
      },
      "args": {}
    },
